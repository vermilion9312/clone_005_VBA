Attribute VB_Name = "mod양력음력변환"
'------------------------------------------------------------------------------------------
'   【 길벗 매크로와 VBA / 도서출판 길벗 / 이동숙(bofb@naver.com) 】
'   기능 :  1901년부터 2030년까지의 음력을 양력으로
'            양력을 음력으로 음력을 양력으로 변환
'            음력을 양력으로 → fn음력to양력(년도 As Integer, 월 As Integer, 일 As Integer, _
'                                                     Optional 윤년구분 As Boolean = False)
'            양력을 음력으로 → fn양력to음력(년도 As Integer, 월 As Long, 일 As Long)
'------------------------------------------------------------------------------------------

Option Explicit

Dim EM(1 To 130, 1 To 13) As Integer, MT(12) As Integer
Dim VM(6) As Integer
Dim CY As Integer, CM As Integer, CD As Integer, CA As Boolean

'------------------------------------------------------------------------------------------
' 음력 날짜를 양력 날짜로 변경해 주는 함수
'------------------------------------------------------------------------------------------
Function fn음력to양력(년도 As Integer, 월 As Integer, 일 As Integer, _
          Optional 윤년구분 As Boolean = False) As Date
Attribute fn음력to양력.VB_Description = "1901년부터 2030년까지의 음력을 양력으로 변환\n[윤년구분]이 True이면 윤년으로, 생략하거나 False를 입력하면 평년으로 계산"
Attribute fn음력to양력.VB_ProcData.VB_Invoke_Func = " \n14"
    Application.Volatile
    
    On Error GoTo ErrorLun
    Dim Y As Integer
    Dim T As Long, Y1 As Long, Y2 As Long, i As Integer, SY As Single, SM, SD, SW As Integer
    
    Y = 년도 - 1900
    Call CalenderData
    T = 일 + 49   ' 기준해인 1901년은 음력 1월 1일이 양력보다 49일 늦다
    
    For i = 1 To Y - 1   ' 전해까지의 음력일수를 구한다
        T = T + EM(i, 13)
    Next i
    
    For i = 1 To 월 - 1    '전월까지의 음력일수를 계산한다
        T = T + VM(EM(Y, i))
    Next i
    
    If 윤년구분 = True And EM(Y, 월) < 3 Then   '윤달이 맞는지 확인한다
       MsgBox "이 달은 윤달이 아닙니다."
       CY = 0: CM = 0: CD = 0
       Exit Function
    End If

    If 윤년구분 = True Then     '윤달인 경우 윤달 일수를 계산한다
       If EM(Y, 월) = 3 Or EM(Y, 월) = 4 Then
          T = T + 29
       Else
          T = T + 30
       End If
    End If
    
    ' 양력이 4년에 한번씩 윤년이 있으므로 계산할 음력해까지
    ' 몇번의 윤년이 있는가 알기위해 음력의 누적일수를 양력 4년 동안의 일수로 나눈 몫을 구한다.
   
    fn음력to양력 = CDate(T + 366)
    
    Exit Function

ErrorLun:
    Exit Function
End Function

'------------------------------------------------------------------------------------------
' 양력 날짜를 입력받아 음력날짜로 변경해줌
'------------------------------------------------------------------------------------------

Function fn양력to음력(양력날짜 As Date)
Attribute fn양력to음력.VB_Description = "1901년부터 2030년까지의 양력을 음력으로 변환"
Attribute fn양력to음력.VB_ProcData.VB_Invoke_Func = " \n14"
   Application.Volatile
   
    On Error GoTo ErrorSun
    '양력날짜를 음력날짜로 바꾼다
    Dim T As Long
    Dim LY As Single
    Dim LM As Integer
    Dim LD As Integer
    Dim Y As Long
    Dim 년도 As Integer, 월 As Integer, 일 As Integer
    
    년도 = Year(양력날짜)
    월 = Month(양력날짜)
    일 = Day(양력날짜)
    Y = 년도 - 1900
    Call CalenderData  '음력 데이타를 불러온다
    '양력날짜를 합산한다
    T = 365 * (Y - 1) + MT(월 - 1) + 일 + Int(Y / 4)
    If Y = 4 * Int(Y / 4) And 30 * 월 + 일 < 90 Then
       T = T - 1
    End If
    T = T - 49 '2001년은 음력이 양력보다 49일 늦으므로 1월 1일을 맞추기 위래 49를 뺀다
    LY = 1
    '음력해를 찾는다
    Do While (T > EM(LY, 13))
       T = T - EM(LY, 13)
       LY = LY + 1
    Loop
    '음력월을 찾는다
    LM = 1
    Do While (T > VM(EM(LY, LM)))
       T = T - VM(EM(LY, LM))
       LM = LM + 1
    Loop
    LD = T
    CY = LY + 1900: CM = LM: CD = LD
    '윤달인지 검사
    If (EM(LY, LM) = 3 Or EM(LY, LM) = 4) And T > 29 Then
       CD = T - 29: CA = True
    ElseIf (EM(LY, LM) = 5 Or EM(LY, LM) = 6) And T > 30 Then
       CD = T - 30: CA = True
    Else: CA = False
    End If
    If CA = True Then                               '윤달인경우 윤달을 표시하기 위한 조건문
        fn양력to음력 = CY & "-" & Format(CM, "00") & "-" & Format(CD, "00") & "(윤달)"
        Else
        fn양력to음력 = CY & "-" & Format(CM, "00") & "-" & Format(CD, "00")
        End If
    Exit Function
ErrorSun:
    Exit Function
End Function

